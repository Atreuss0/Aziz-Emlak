    <link rel="stylesheet" href="~/css/Panel/Panellayout.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <title>Aziz Emlak Yonetim Paneli</title>

       <script src="~/js/tinymce/tinymce.min.js"></script>
       <script >   
           tinymce.init({
               selector: '#myTextarea',
               width:1000,        
           });
       </script>  

</head>
<body>
    <div class="header-container">
        <div class="header">
            <nav class="navbar">
                <div class="nav-logo">
                    <a href="~/Home/Index">
                        <img src="~/Media/StaticImage/logo.png" alt="aziz emlak logo">
                    </a>
                </div>
                <div class="navbar-item">
                    <ul class="navbar-links">
                        <li><a class="nav-link" href="~/Home/Index">ANASAYFA</a></li>
                        <li><a class="nav-link" href="~/Home/kurumsal">KURUMSAL</a></li>
                        <li><a class="nav-link" href="~/Home/Portfoyler">PORTFÖYLER</a></li>
                        <li><a class="nav-link" href="~/Home/Danısmanlar">DANIŞMANLAR</a></li>
                        <li><a class="nav-link" href="~/Home/Iletisim">İLETİŞİM</a></li>
                    </ul>

                </div>
                <div class="burger" onclick="genislet()">
                    <div class="line1"></div>
                    <div class="line2"></div>
                    <div class="line3"></div>
                </div>
            </nav>

        </div>
    </div>

    <div class="container">

        <div class="panel">
            <a href="~/Home/Index">
                <img src="~/Media/StaticImage/logo.png" alt="aziz emlak logo">
            </a>
            <div class="satir">
                <li><a class="panela" href="~/Panel/Index"><i class="fa-solid fa-user"></i> PROFİLİM</a></li>
                <li><a class="panela" href="~/Panel/Portfoylerim"><i class="fa-solid fa-building"></i> PORTFOYLERİM</a></li>
