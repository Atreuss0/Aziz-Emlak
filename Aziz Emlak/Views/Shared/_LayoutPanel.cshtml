                <li><a class="panela" href="~/Panel/Mesajlar"><i class="fa-solid fa-message"></i> MESAJLAR</a></li>
                <li><a class="panela" href="~/Panel/Ayarlar"><i class="fa-solid fa-gear"></i> AYARLAR</a></li>
                <li><a class="panela" href="~/Panel/Logout"><i class="fa-solid fa-arrow-right-from-bracket"></i> Cıkış Yap</a></li>
            </div>
        </div>

        @RenderBody()

    </div>

    <script src="~/js/AdminMobile.js"></script>

    <script src="https://cdn.ckeditor.com/ckeditor5/36.0.1/classic/ckeditor.js"></script>
  
     
